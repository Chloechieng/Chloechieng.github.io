{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Real Interest Rate (%) in the US overtime",
  "data": {
    "url": "https://raw.githubusercontent.com/Chloechieng/Chloechieng.github.io/main/week%208...csv"
  },
  "title": {
    "text": "US Interest Rates",
    "subtitle": [
      "Real US Interest Rate(%) from 1961-202, Data source: The World Bank"
    ],
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "mark": {"type": "line", "point": false, "color": "#4b33ce94"},
  "width": 300,
  "height": 270,
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal",
      "axis": {"title": null, "grid": false, "ticks": false, "labelAngle": 45}
    },
    "y": {
      "field": "Real interest rate (%)",
      "type": "quantitative",
      "title": "Real Interest Rate (%)",
      "axis": {"grid": false}
    },
    "tooltip": [
      {"field": "Year", "type": "temporal", "title": "Year"},
      {
        "field": "Real interest rate (%)",
        "type": "nominal",
        "title": "Real interest rate (%)"
      }
    ]
  },
  "config": {}
}