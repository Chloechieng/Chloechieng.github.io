{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/Chloechieng/Chloechieng.github.io/main/data_ONS-AIYE%20.json",
      "format": {"type": "json", "property": "years"}
    },
    "title": {
      "text": "Inactive females in full time edcuation, UK",
      "subtitle": "Aged 16-24 from 1992-2021, Source: ONS",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "color": "black"
    },
    "width": 200,
    "height": 200,
    "mark": {"type": "line", "color": "rgb(0,47,167"},
    "encoding": {
      "x": {
        "field": "date",
        "type": "temporal",
        "axis": {"grid": false},
        "title": ""
      },
      "y": {
        "field": "value",
        "type": "quantitative",
        "axis": {"grid": false},
        "title": "Number of females (000's)"
      }
    },

      "transform": [
        {"filter": "datum.year>minYear"},
        {"filter": "datum.year<maxYear"}
      ],
      "params": [
        {
          "name": "minYear",
          "value": 2010,
          "bind": {
            "input": "range",
            "min": 1971,
            "max": 2022,
            "step": 1,
            "name": "Start year:"
          }
        },
        {
          "name": "maxYear",
          "value": 2022,
          "bind": {
            "input": "range",
            "min": 1971,
            "max": 2022,
            "step": 1,
            "name": "End year:"
          }
        }
      ]
    }
    
  